    <scriptTask id="scripttask1" name="FilterPeritos" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var peritosAll = JSON.parse(execution.getVariable('registeredUsersPeritos'));
var peritosSelected = JSON.parse(execution.getVariable('choosenPeritos'));
var peritosParsed = [];
for (var index = 0; index &lt; peritosAll.length; index++) {
    var bool = false;
    for (var index2 = 0; index2 &lt; peritosSelected.length; index2++) {
        if(peritosAll[index].text == peritosSelected[index2].text){
            bool = true;
        }
    }
    if(bool == false){
        peritosParsed.push(peritosAll[index]);
    }
}
print(JSON.stringify(peritosParsed));
execution.setVariable('registeredUsersPeritos', JSON.stringify(peritosParsed));</script>
