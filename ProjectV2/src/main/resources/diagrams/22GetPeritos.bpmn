      <script>var peritosSelected = execution.getVariable('peritos1');
var splitedArray = peritosSelected.split(',');
var choosenPeritos = [];
for (var i = 0; i &lt; splitedArray.length; i++) {
    choosenPeritos.push({_id: splitedArray[i], text: splitedArray[i]});
}
execution.setVariable('choosenPeritos', JSON.stringify(choosenPeritos));</script>
